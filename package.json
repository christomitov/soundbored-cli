{
  "name": "soundbored",
  "version": "0.1.3",
  "private": false,
  "type": "module",
  "description": "CLI tool for fuzzy searching and triggering sounds from SoundBored API",
  "main": "dist/cli.js",
  "bin": {
    "soundbored": "bin/soundbored"
  },
  "scripts": {
    "build": "node build.js",
    "start": "node dist/cli.js",
    "dev": "node --enable-source-maps dist/cli.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "release:prep": "node scripts/make-release.mjs",
    "release:publish": "npm run build && node scripts/make-release.mjs && npm publish ./release --access public"
  },
  "publishConfig": {},
  "files": [
    "dist/cli.js",
    "bin/soundbored"
  ],
  "keywords": [
    "cli",
    "sounds",
    "search",
    "fuzzy"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.4.7",
    "fuse.js": "^7.0.0",
    "fuzzy": "^0.1.3",
    "ink": "^6.2.2",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^8.2.6",
    "inquirer-autocomplete-prompt": "^2.0.1",
    "react": "^19.1.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/inquirer-autocomplete-prompt": "^3.0.0",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.0",
    "@types/react": "^19.1.10",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "esbuild": "^0.25.9",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "react-devtools-core": "^4.28.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.4",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
